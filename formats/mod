.mod file format (little endian)

"MOD"
#mod metadata section
DWORD chunklength
STRING name
DWORD version
BYTE authors
[authors]
	STRING author
BYTE type
	0 - package
	1 - world
	2 - generator
	3 - meta
	4 - viral
	5 - content
	6 - resource
BIT(8) permission
	0 - read map
	1 - write to map
	2 - player
	3 - input
	4 - overlay
	5 - render
BYTE mods_modding
[mods_modding]
	STRING mod_name
"ERC" #Embedded ResourCes
DWORD chunklength
<compressed file archive>
"CODE"
#Python code section
DWORD chunklength
WORD codebits
[codebits]
	<bytestream for code object>
"BLOX"
#Block definition section
DWORD chunklength
WORD blocks
[blocks]
	STRING name
	STRING description
	FLOAT light
	FLOAT gravity
	WORD code
	STRING mesh
	STRING skin
"CHEK"
#Checksum of the file to make sure there were no errors
<SHA-512 checksum>