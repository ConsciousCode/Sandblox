<?xml version="1.0" encoding="utf-8"?>
<mod>
	<!-- The name of the mod -->
	<name>Default Mod</name> 
	<!-- A description of the mod -->
	<description>The default, preincluded mod for Sandblox</description>
	<!-- The mod's version -->
	<version>0.1.0.0</version>
	<!-- The comma-separated authors of the mod -->
	<authors>
		<author name="Robert McDaniels" contrib="Everything"/>
	</authors>
	<!-- The permissions requested by the mod -->
	<permissions>
		<!-- Read access to the map
		<readmap/> -->
		<!-- Write access to the map (if not of type Generator)
		<writemap/> -->
		<!-- Read/write access to player attributes -->
		<player/>
		<!-- Access to player input -->
		<input/>
		<!-- Access to the overlay
		<overlay/> -->
		<!-- Access to direct rendering capabilities
		<render/> -->
	</permissions>
	<!-- Lists mods the meta mod can edit (if targeting specific mods)
	<meta>List of mods</meta> -->
</mod>